    {
      "enabled": true,
      "id": "weekly_bootstrap",
      "interval_minutes": 10080,
      "jitter_seconds": 300,
      "label": "Weekly bootstrap",
      "requires_mnemonic": false,
      "steps": [
        "discovery",
        "watchlists",
        "recommendations",
        "downloads",
        "news",
        "training"
      ]
    }
  ],
  "training": {
    "batch_size": 16,
    "enabled": true,
    "epochs": 1
  }
